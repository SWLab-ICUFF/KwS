drop graph kwsg:new_seeds;
drop graph kwsg:subgraphs;
drop graph kwsg:groups;
drop graph kwsg:groups2;

#ver isso com Luiz Andr√©
DELETE {graph ?g {?s ?p ?o} }
WHERE{
  	SELECT ?g ?s ?p ?o
  	WHERE{
  		graph ?g{
                    ?s ?p ?o.
                    ?g kws:coverageScore ?coverageScore
                    FILTER(?coverageScore < 0.80)
                }
  	}
	
};

DELETE {graph ?g {?s ?p ?o} }
WHERE{
  	SELECT ?g ?s ?p ?o
  	WHERE{
  		graph ?g{
                    ?s ?p ?o.
                    ?g kws:harmonicScore ?harmonicScore.
                    FILTER(?harmonicScore < 0.65)
                }
  	}
	
};



#inserir tempo no grafo default
insert data {
    <%1$squery> a kws:Information;
        kws:keywordQuery '%2$s';
        kws:duration [
            a time:Duration;
            time:numericDuration "%3$s"^^xsd:decimal;
            time:unityType time:unitSecond
        ].
};




